#include <stdio.h>
#include <stdlib.h>

int main() {
    int element[10];
    int i;
    printf("Enter 10 elements:\n"); 
    for (i = 0; i < 10; i++) {
        printf("%d=", i + 1);
        scanf ("%d",&element[i]);
        }
    printf("\nThe elements you entered are:\n");
    for (i = 0; i < 10; i++) {
        printf("Element %d: %d\n", i + 1, element[i]);
    }
    return 0;
}
